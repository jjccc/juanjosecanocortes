<div class="container">
  <% @contacts.each do |contact| %>
    <div class="row">
      <p><%= contact.created_at %></p>
      <p><%= contact.name %></p>
      <p><%= contact.email %></p>
      <p><%= contact.phone %></p>
      <p><%= contact.message %></p>
      <hr>
    </div>
  <% end %>
</div>